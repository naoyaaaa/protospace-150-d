<div class="main">
  <div class="inner">
    <div class="form__wrapper">
      <h2 class="page-heading">プロトタイプ編集</h2>
        <%= form_with(model: @prototype, local: true) do |form| %>
          <%= form.text_field :title, placeholder: "title" %>
          <%= form.text_field :catch?catch_copy, placeholder: "catch_copy" %>
          <%= form.text_field :concept, placeholder: "concept"%>
          <%= form.text_field :user, placeholder: "user" %>
          <%= form.submit "SEND" %>
        <% end %>
    </div>
  </div>
</div>